{
  "name": "imedge/inventory-feature",
  "type": "project",
  "config": {
    "platform": {
      "php": "8.1.18"
    },
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IMEdge\\InventoryFeature\\": "src"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "imedge/db-migration": ">=0.1",
    "imedge/log": ">=0.7.1",
    "imedge/node-provided-libs": "^0.9.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10 | ^2",
    "phpunit/phpunit": "^10.3",
    "squizlabs/php_codesniffer": "^3.7",
    "symplify/easy-coding-standard": "^12.0"
  }
}
